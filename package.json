{
  "name": "relango",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "@reduxjs/toolkit": "1.7.2",
    "@testing-library/jest-dom": "5.16.2",
    "@testing-library/react": "12.1.3",
    "@testing-library/user-event": "13.5.0",
    "@types/jest": "27.4.1",
    "@types/node": "16.11.25",
    "@types/react": "17.0.39",
    "@types/react-dom": "17.0.11",
    "antd": "4.18.8",
    "connected-react-router": "6.9.2",
    "formik": "2.2.9",
    "history": "5.3.0",
    "node-sass": "7.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-markdown": "8.0.0",
    "react-redux": "7.2.6",
    "react-router": "5.2.1",
    "react-router-dom": "5.3.0",
    "react-scripts": "5.0.0",
    "typescript": "4.5.5",
    "url-parse": "1.5.10",
    "uuid": "8.3.2",
    "zen-observable": "0.8.15"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "rimraf ./build && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prettier": "prettier --write \"./src/{,!(node_modules|dist|build|coverage)/**/}*.{js,jsx,ts,tsx,json}\"",
    "pre-commit": "lint-staged",
    "lint": "tsc --noEmit && eslint ./src/**/*.{js,jsx,ts,tsx}",
    "lint:fix": "eslint --fix ./src/**/*.{js,jsx,ts,tsx}",
    "firebase:deploy": "npm run build && firebase deploy --only hosting"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pre-commit"
    }
  },
  "devDependencies": {
    "@types/history": "4.7.11",
    "@types/react-redux": "7.1.22",
    "@types/react-router": "5.1.18",
    "@types/react-router-dom": "5.3.3",
    "@types/url-parse": "1.4.8",
    "@types/uuid": "8.3.4",
    "@types/webpack-env": "1.16.3",
    "@types/zen-observable": "0.8.3",
    "eslint": "8.9.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.4.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react-hooks": "4.3.0",
    "husky": "7.0.4",
    "lint-staged": "12.3.4",
    "prettier": "2.5.1",
    "rimraf": "3.0.2"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less|sass|scss)$": "<rootDir>/__mocks__/styleMock.js",
      "\\.(gif|ttf|eot|svg)$": "<rootDir>/__mocks__/fileMock.js"
    },
    "transformIgnorePatterns": [
      "(?!(/node_modules/(react-markdown|vfile|vfile-message|unist-.*|unified|bail|is-plain-obj|trough|remark-.*|mdast-util-.*|micromark.*|decode-named-character-reference|character-entities|property-information|hast-util-whitespace|space-separated-tokens|comma-separated-tokens|pretty-bytes)/))(/node_modules/.+.(js|jsx|mjs|cjs|ts|tsx)$)"
    ]
  }
}
