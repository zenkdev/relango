{
  "name": "relango",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "@craco/craco": "6.4.4",
    "@reduxjs/toolkit": "1.8.3",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "13.3.0",
    "@testing-library/user-event": "14.2.1",
    "@types/jest": "28.1.4",
    "@types/node": "16.11.43",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "antd": "4.21.5",
    "chevrotain": "^10.1.2",
    "craco-antd": "^2.0.0",
    "formik": "2.2.9",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-markdown": "8.0.3",
    "react-redux": "8.0.2",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "redux": "^4.2.0",
    "sass": "1.53.0",
    "typescript": "4.7.4",
    "url-parse": "1.5.10",
    "uuid": "8.3.2",
    "web-vitals": "^2.1.4",
    "workbox-background-sync": "^6.5.3",
    "workbox-broadcast-update": "^6.5.3",
    "workbox-cacheable-response": "^6.5.3",
    "workbox-core": "^6.5.3",
    "workbox-expiration": "^6.5.3",
    "workbox-google-analytics": "^6.5.3",
    "workbox-navigation-preload": "^6.5.3",
    "workbox-precaching": "^6.5.3",
    "workbox-range-requests": "^6.5.3",
    "workbox-routing": "^6.5.3",
    "workbox-strategies": "^6.5.3",
    "workbox-streams": "^6.5.3",
    "yaml": "2.1.1",
    "zen-observable": "0.8.15"
  },
  "scripts": {
    "start": "craco start",
    "build": "rimraf ./build && craco build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prettier": "prettier --write \"./src/{,!(node_modules|dist|build|coverage)/**/}*.{js,jsx,ts,tsx,json}\"",
    "pre-commit": "lint-staged",
    "lint": "tsc --noEmit && eslint ./src/**/*.{js,jsx,ts,tsx}",
    "lint:fix": "eslint --fix ./src/**/*.{js,jsx,ts,tsx}",
    "gen_diagrams": "cd scripts && ts-node gen_diagrams.ts",
    "gen_dts_signatures": "cd scripts && ts-node gen_dts_signatures.ts"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run pre-commit"
    }
  },
  "devDependencies": {
    "@types/history": "4.7.11",
    "@types/react-redux": "7.1.24",
    "@types/react-router": "5.1.18",
    "@types/react-router-dom": "5.3.3",
    "@types/url-parse": "1.4.8",
    "@types/uuid": "8.3.4",
    "@types/webpack-env": "1.17.0",
    "@types/zen-observable": "0.8.3",
    "eslint": "8.19.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "^8.0.1",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "rimraf": "3.0.2"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less|sass|scss)$": "<rootDir>/__mocks__/styleMock.js",
      "\\.(gif|ttf|eot|svg)$": "<rootDir>/__mocks__/fileMock.js"
    },
    "transformIgnorePatterns": [
      "(?!(/node_modules/(react-markdown|vfile|vfile-message|unist-.*|unified|bail|is-plain-obj|trough|remark-.*|mdast-util-.*|micromark.*|decode-named-character-reference|character-entities|property-information|hast-util-whitespace|space-separated-tokens|comma-separated-tokens|pretty-bytes)/))(/node_modules/.+.(js|jsx|mjs|cjs|ts|tsx)$)"
    ]
  }
}
